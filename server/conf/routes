# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                          controllers.Application.index

GET        /register                  controllers.Application.register

GET        /streams                   controllers.StreamController.streams

GET        /back-pressured-stream     controllers.StreamController.backPressuredStream(periodMillis: Int, seed: Long)
GET        /simple-stream             controllers.StreamController.simpleStream(periodMillis: Int, seed: Long)

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file              controllers.Assets.at(path="/public", file)
