# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                          controllers.Application.index
GET        /logout                    controllers.Application.logout

GET        /login                     controllers.RegisterController.getLogin
POST       /login                     controllers.RegisterController.login
GET        /register                  controllers.RegisterController.getRegister
POST       /register                  controllers.RegisterController.register
GET        /register/all              controllers.RegisterController.all

GET        /streams                   controllers.StreamController.streams

GET        /back-pressured-stream     controllers.StreamController.backPressuredStream(periodMillis: Int, seed: Long)
GET        /simple-stream             controllers.StreamController.simpleStream(periodMillis: Int, seed: Long)

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file              controllers.Assets.versioned(path="/public", file)
